    <xs:sequence>
      <xs:element minOccurs="0" name="return" type="xs:string" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="obtenerComentario">
    <xs:sequence>
      <xs:element name="idComentario" type="xs:int" />
